{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 240
    },
    "summary": {
      "type": "string",
      "required": true,
      "maxLength": 90
    },
    "Images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
